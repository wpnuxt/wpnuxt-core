{
  "name": "@vernaillen/wpnuxt",
  "version": "0.1.7",
  "description": "WPNuxt",
  "repository": "vernaillen/wpnuxt-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run prepack",
    "prepack": "nuxt-module-build build",
    "dev": "pnpm --filter ./docs/ run build && pnpm --filter ./playground/ run dev",
    "dev:build": "pnpm --filter ./playground/ run build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs:build": "cd docs && nuxi build",
    "docs:dev": "cd docs && nuxi dev",
    "release": "npm run lint &&  npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@nuxt/kit": "^3.9.0",
    "@vueuse/core": "^10.7.1",
    "defu": "^6.1.3",
    "nuxt-graphql-middleware": "^4.0.0-beta.9",
    "nuxt-multi-cache": "^3.1.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.1.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.0",
    "@nuxt/test-utils": "^3.9.0",
    "@types/node": "20.10.5",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/test-utils": "^2.4.3",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.0",
    "vitest": "^1.1.0",
    "vitest-environment-nuxt": "^1.0.0"
  }
}